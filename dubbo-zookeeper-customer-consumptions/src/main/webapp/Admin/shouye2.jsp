<script type="text/javascript">

    window.isWide = (window.innerWidth || document.body.clientWidth || document.documentElement.clientWidth) >= 1280 ? true : false;


    var Ie8 = /msie\s8\.0/.test(navigator.userAgent.toLowerCase());
    window.isWide = window.isWide && !Ie8;

    function changeSrc(id) {
        if (!document.getElementById(id)) {
            return;
        }

        var imgObj = document.getElementById(id).getElementsByTagName("img");

        if (!imgObj) {
            return;
        }

        var len = imgObj.length;
        for (var i = 0; i < len; i++) {
            if (!imgObj[i].getAttribute("src_w")) {
                continue;
            }

            var src = imgObj[i].getAttribute("src").indexOf("transparent.png") > 0 || imgObj[i].getAttribute("src") ==  "",
                    srcN = imgObj[i].getAttribute("src_n"),
                    srcW = imgObj[i].getAttribute("src_w"),
                    dataSrc = imgObj[i].getAttribute("data-src") == "";
            if (id !== 'UED_FocusImage' || imgObj[i].getAttribute('class') == 'small_banner_img') {
                srcN = retinaImg(srcN);
                srcW = retinaImg(srcW);
                imgObj[i].setAttribute("src_n", srcN);
                imgObj[i].setAttribute("src_w", srcW);
            }

            if (window.isWide) {
                if (src) {
                    imgObj[i].setAttribute("src", srcW);
                } else if (dataSrc) {
                    imgObj[i].setAttribute("data-src", srcW);
                }
            } else {
                if (src) {
                    imgObj[i].setAttribute("src", srcN);
                } else if (dataSrc) {
                    imgObj[i].setAttribute("data-src", srcN);
                }
            }
        }
    }

    // changeSrc("UED_SmallBanners");
    changeSrc("UED_FocusImage");
    changeSrc("brandSale");
    changeSrc("hotSaleIndex");

    function removeLoading(parentId, childId) {
        var parent = document.getElementById(parentId),
                child = document.getElementById(childId)
        if (!parent || !child) {
            return;
        }
        parent.removeChild(child);
    }
    // description retina屏幕下将图片地址信息中的尺寸信息加倍
    function retinaImg(img){
        if (!!window && window.devicePixelRatio > 1) {
            var arr = img.match(/\d+x\d+|\d+x|x\d+/g);
            if (!!arr) {
                var len = arr.length;
                if (len > 1) {
                    var size = arr[len - 1].split('x'),
                            newW = parseInt(size[0]) * 2 || "",
                            newH = parseInt(size[1]) * 2 || "",
                    //原图尺寸
                            oldSize = arr[0].split('x'),
                            oldW = parseInt(oldSize[0]),
                            oldH = parseInt(oldSize[1]);
                    //加倍后的尺寸必须小于原图的尺寸
                    if (newW <= oldW && newH <= oldH) {
                        //将新的尺寸替换符合arr[len - 1]这个条件的尺寸
                        img = img.replace(new RegExp(arr[len - 1]+'(?!.*'+arr[len - 1]+')'), newW + 'x' + newH);
                    }
                }
            }
        }
        return img;
    }

    removeLoading("rigt_wrapper", "rigt_wrapper_loading");
    removeLoading("brandAds", "brandSale_loading");
    removeLoading("hotAds", "hotSaleIndex_loading");
</script>

