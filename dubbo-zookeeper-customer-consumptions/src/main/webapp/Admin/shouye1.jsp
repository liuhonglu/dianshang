<script type="text/javascript">
    function judge(req) {
        var host = req.host, pathName = req.pathname;
        //品牌团页面
        if (host.indexOf('brand') > -1 ) {
            return '//brand.zhe800.com/';
        }
        if (pathName.indexOf('youpinhui') > -1 ) {
            return '//www.zhe800.com/n/youpinhui';
        }
        if (pathName.indexOf('baoyou') > -1 ) {
            return '//www.zhe800.com/ju_type/baoyou';
        }
        if (pathName.indexOf('jingxuanyugao') > -1 ) {
            return '//www.zhe800.com/jingxuanyugao';
        }
        //排行榜页面
        if (host.indexOf('top') > -1 ) {
            return '//top.zhe800.com/';
        }
        if (host.indexOf('jifen') > -1 ) {
            return '//jifen.zhe800.com/';
        }
        if (pathName.indexOf('taojingxuan') > -1 ) {
            return '//www.zhe800.com/cn/list/taojingxuan';
        }
        /* //今日上新
         if (host.indexOf('new') > -1 && curr == 'new') {
         return className;
         }
         //最后疯抢
         if (host.indexOf('last') > -1 && curr == 'last') {
         return className;
         }*/
        //首页选中特殊处理
        if (/^(\/?|\/page\/\d+)(\/?)$/.test(pathName) && host.indexOf('www') > -1) {
            return '//www.zhe800.com';
        }

        //多子页面情况
        if (host.indexOf('www') > -1) {
            var indexUrl=['meirishijian', 'tmall', 'ju_tag'];
            var i = 0, len = indexUrl.length;
            for (i; i < len; i++) {
                if (pathName.indexOf(indexUrl[i]) > -1) {
                    return '//www.zhe800.com';
                }
            }
            return '';
        }
        return '';
    }
    function navAddClass(url){
        if(url) {
            var nav_hrefs = document.getElementById('head_nav').getElementsByTagName('a');
            for (var index = 0; index < nav_hrefs.length; index++) {
                if (nav_hrefs[index].getAttribute('href') === url) {
                    nav_hrefs[index].className += " " + 'on';
                }
            }
        }
    }
    var currentUrl=judge(window.location);
    navAddClass(currentUrl);
</script>



